{
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "tabWidth": 2,
    "printWidth": 80,
